<?php

/**
 * @file
 * Contains training_entity_config_auto.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function training_entity_config_auto_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the training_entity_config_auto module.
    case 'help.page.training_entity_config_auto':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('My Awesome Module') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function training_entity_config_auto_theme() {
  return [
    'training_entity_config_auto' => [
      'render element' => 'children',
    ],
  ];
}
